{
  "service": {
    "name": "voca-connect",
    "version": "1.0.0",
    "port": 8001
  },
  "aws": {
    "region": "us-east-1",
    "connect": {
      "instance_prefix": "voca-connect",
      "flow_prefix": "VocaFlow"
    },
    "lambda": {
      "function_prefix": "voca-lambda",
      "runtime": "python3.11",
      "timeout": 30
    }
  },
  "api": {
    "voca_ai_engine_url": "http://voca-ai-engine:8008",
    "timeout": 30000
  },
  "logging": {
    "level": "info",
    "format": "json"
  }
}
